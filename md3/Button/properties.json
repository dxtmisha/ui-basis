{
  "basic": "{d.button}",
  "padding": {
    "_type": "var"
  },
  "height": {
    "_default": "md"
  },
  "color": {
    "_rename": "palette-color"
  },
  "gradient": {
    "_rename": "palette-gradient"
  },
  "#inscription": {
    "column-gap": "{ref.spacing.2}",
    "padding-left": "{??padding}",
    "padding-right": "{??padding}",
    "text-overflow": "truncate"
  },
  "filled": {
    "color": {
      "_rename": "color"
    },
    "background-color": {
      "_rename": "palette-background"
    },
    "_category": "appearance",
    "_default": true
  },
  "outlined": {
    "before": {},
    "after": {
      "basic": {},
      "border-color": {
        "_rename": "palette-border"
      }
    },
    "_category": "appearance"
  },
  "text": {
    "_category": "appearance"
  },
  "elevated": {
    "background-color": {
      "_rename": "palette-background"
    },
    "_category": "appearance"
  },
  "tonal": {
    "color": {
      "_rename": "color"
    },
    "background-color": {
      "_rename": "palette-background"
    },
    "_category": "appearance"
  },
  "@@palette": "{palette}"
}
