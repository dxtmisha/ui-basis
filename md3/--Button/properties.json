{
  "basic": "{d.button}",
  "padding": {
    "_replace": "^.*? (.*?)$"
  },
  "height": {
    "_default": "md"
  },
  "color": {
    "_rename": "palette-color"
  },
  "gradient": {
    "_rename": "palette-gradient"
  },
  "filled": {
    "color": {
      "_rename": "color"
    },
    "background-color": {
      "_rename": "palette-background"
    },
    "gradient": {
      "_rename": "gradient"
    },
    "_category": "appearance",
    "_default": true
  },
  "outlined": {
    "_category": "appearance"
  },
  "text": {
    "_category": "appearance"
  },
  "elevated": {
    "background-color": {
      "_rename": "palette-background"
    },
    "_category": "appearance"
  },
  "tonal": {
    "color": {
      "_rename": "color"
    },
    "background-color": {
      "_rename": "palette-background"
    },
    "gradient": {
      "_rename": "gradient"
    },
    "_category": "appearance"
  },
  "@@palette": "{palette}"
}
