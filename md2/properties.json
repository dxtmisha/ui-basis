{
  "ref": {
    "value": {
      "file|main": "tokens/ref.json",
      "screen": {
        "_category": "media"
      },
      "shade": {
        "_category": "shade"
      },
      "palette": {
        "_category": "palette"
      }
    },
    "_variable": "var",
    "_category": "root"
  },
  "sys": {
    "value": {
      "file|main": "tokens/sys.json"
    },
    "_variable": "var",
    "_category": "root"
  },
  "elevation": {
    "value": {
      "file|main": "tokens/elevation.json"
    },
    "_variable": "state",
    "_category": "class"
  },
  "grid": {
    "value": {
      "file|main": "tokens/grid.json"
    },
    "_variable": "state",
    "_category": "class"
  },
  "font": {
    "value": {
      "file|main": "tokens/font.json"
    },
    "_variable": "state",
    "_category": "class"
  },
  "rounded": {
    "value": {
      "file|main": "tokens/rounded.json"
    },
    "_variable": "state",
    "_category": "class"
  },
  "button": {
    "padding-left": "{ref.spacing.4}",
    "padding_right": "{ref.spacing.4}",
    "height": {
      "sm": "{ref.spacing.7}",
      "md": "{ref.spacing.9}",
      "lg": "{ref.spacing.12}",
      "border-color": "{ref.spacing.2}"
    },
    "font": "{font.button}",
    "border_radius": "{ref.spacing.1}",
    "contained": {
      "color": "{sys.palette.onPrimary}",
      "background-color": {
        "value": "{sys.palette.primary}",
        "_rename": "palette-background"
      },
      "height": {
        "xl": "{ref.spacing.16}"
      },
      "hover": {
        "gradient": "{sys.palette.onPrimary}",
        "gradient-opacity": "{sys.opacity.hover}"
      },
      "disabled": {
        "opacity": "{sys.opacity.disabled}"
      }
    },
    "outlined": {
      "color": {
        "value": "{sys.palette.primaryVariant}",
        "_rename": "palette-background"
      },
      "border-color": "{sys.palette.outline}",
      "border-opacity": "{sys.opacity.outline}",
      "border-width": "1px"
    },
    "text": {
      "color": "{sys.palette.primary}",
      "palette-background": "{ref.palette.red.300}"
    }
  }
}
