{
  "basic": "{d.button}",
  "padding": {
    "_type": "var"
  },
  "height": {
    "_default": "md"
  },
  "color": {
    "_rename": "palette-color"
  },
  "gradient": {
    "_rename": "palette-gradient"
  },
  "#inscription": {
    "padding-left": "{??padding}",
    "padding-right": "{??padding}"
  },
  "dragged": {
    "_type": "state"
  },
  "contained": {
    "background-color": {
      "_rename": "palette-background"
    },
    "_category": "appearance",
    "_default": true
  },
  "outlined": {
    "_category": "appearance"
  },
  "text": {
    "_category": "appearance"
  },
  "rounded": {
    "_default": "standard"
  },
  "@@palette": "{palette}"
}
