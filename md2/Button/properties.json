{
  "&.is-test": {
    "color": "red"
  },
  "& .??-test": {
    "color": "blue"
  },
  "& .?-test": {
    "color": "white"
  },
  "animate": {
    "animate|go1": {
      "0%": {
        "opacity": ".1"
      },
      "40%": {
        "opacity": ".6"
      },
      "100%": {
        "opacity": "1"
      }
    },
    "animate|go2": {
      "0%": {
        "opacity": ".1",
        "font-size": "16px"
      },
      "40%": {
        "opacity": ".6"
      },
      "100%": {
        "opacity": "1",
        "font-size": "24px"
      }
    }
  },
  "font": "{?font.body1}",
  "#text": {
    "color": "white",
    "font-size": "16px",
    "hover": {
      "color": "black"
    }
  },
  "isSt": {
    "color": "white",
    "font-size": "16px",
    "hover": {
      "color": "black"
    }
  },
  "after": {
    "color": "blue",
    "toGo": {
      "color": "red"
    }
  },
  "hover": {
    "color": "blue",
    "toGo": {
      "color": "red",
      "toGo2": {
        "color": "green",
        "focus": {
          "border-color": "blue"
        }
      }
    }
  },
  "first-child": {
    "color": "blue",
    "toGo": {
      "color": "red",
      "toGo2": {
        "color": "green",
        "focus": {
          "border-color": "blue"
        }
      }
    }
  },
  "disabled": {
    "color": "blue",
    "toGo": {
      "color": "red",
      "toGo2": {
        "color": "green",
        "focus": {
          "border-color": "blue"
        }
      }
    }
  },
  "media|{ref.screen.sm}": {
    "color": "green"
  },
  "media-max|{ref.screen.md}": {
    "color": "blue"
  },
  "media|{ref.screen.sm},{ref.screen.md}": {
    "color": "red"
  },
  "var|test": "asd",
  "test2": "asd2",
  "test3": "{??test2}",
  "test31": "{##test2}",
  "test4": {
    "color": {
      "value": "blue",
      "_rename": "background-color"
    },
    "isVar": {
      "value": "blue",
      "_rename": "toVar"
    },
    "isVar2": {
      "value": "blue",
      "_rename": "toVar2"
    },
    "border-color": {
      "value": "blue",
      "_rename": "var|border"
    },
    "font": {
      "value": "12px",
      "_rename": "font-size"
    },
    "update": {
      "color": "green",
      "isVar": "green",
      "isVar2": {
        "value": "green",
        "_rename": "toVar3"
      },
      "border-color": "green",
      "font-big": {
        "font": "16px"
      }
    }
  },
  "test5": "{?checkbox.color}",
  "test6": {
    "value": "{{?checkbox.color}}",
    "_fullValue": true
  },
  "test7": {
    "value": "{is_{?checkbox.color}}",
    "_fullName": true
  },
  "test8": "{is_{?checkbox.color}_end}",
  "=??test9": "is-n",
  "??test91": "is-n",
  "=test10": {
    "a": {
      "value": "v1",
      "_rename": "=??a"
    },
    "b": "v2"
  },
  "not(.asd)": {
    "color": "black"
  },
  "color": "red",
  "color{??test2}-{??test}": "red",
  "border": {
    "value": "red",
    "_variable": "var"
  },
  "var|cursor": {
    "value": "value-cursor"
  },
  "border-color": {
    "color1": "red",
    "color2": "blue",
    "_rename": "var|border"
  },
  "background-color": {
    "a": "red",
    "b": "blue",
    "c": "green"
  },
  "height": {
    "a": "red",
    "b": "blue",
    "c": "green",
    "_rename": "min-height"
  },
  "var|linkName": "toLink",
  "@link1": "{checkbox.toLink}",
  "@link2": "{checkbox.{??linkName}}"
}
