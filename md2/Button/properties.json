{
  "basic": "{d.button}",
  "padding": {
    "_type": "var"
  },
  "height": {
    "_default": "md"
  },
  "color": {
    "_rename": "palette-color"
  },
  "gradient": {
    "_rename": "palette-gradient"
  },
  "#inscription": {
    "column-gap": "{ref.spacing.2}",
    "padding-left": "{??padding}",
    "padding-right": "{??padding}",
    "text-overflow": "truncate"
  },
  "contained": {
    "color": {
      "_rename": "color"
    },
    "background-color": {
      "_rename": "palette-background"
    },
    "_category": "appearance",
    "_default": true
  },
  "outlined": {
    "_category": "appearance"
  },
  "text": {
    "_category": "appearance"
  },
  "adaptive": {
    "sm": {
      "media-max|sm": {
        "basic": {
          "value": "{??adaptive.icon}",
          "_type": "link"
        }
      }
    },
    "md": {
      "media-max|md": {
        "basic": {
          "value": "{??adaptive.icon}",
          "_type": "link"
        }
      }
    },
    "lg": {
      "media-max|lg": {
        "basic": {
          "value": "{??adaptive.icon}",
          "_type": "link"
        }
      }
    }
  },
  "@@palette": "{palette}"
}
